<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <TargetFramework>net6.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <LangVersion>latest</LangVersion>
    <AssemblyTitle>ChaosLauncher</AssemblyTitle>
    <Company>Chaotic Minds</Company>
    <Product>ChaosLauncher</Product>
    <NeutralLanguage>en</NeutralLanguage>
    <Description>A launcher for Chaos, a private Dark Ages server.</Description>
    <Copyright>Copyright ©  2017</Copyright>
    <OutputPath>bin\$(Configuration)\</OutputPath>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <PropertyGroup>
    <ApplicationIcon>chaos.ico</ApplicationIcon>
  </PropertyGroup>
  <PropertyGroup>
    <ApplicationManifest>app.manifest</ApplicationManifest>
  </PropertyGroup>
  <ItemGroup>
    <Compile Update="Forms\MessageDialog.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\MessageDialog.designer.cs">
      <DependentUpon>MessageDialog.cs</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <AutoGen>True</AutoGen>
      <DesignTime>True</DesignTime>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Update="Forms\MainForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\MainForm.Designer.cs">
      <DependentUpon>MainForm.cs</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <None Include="app.manifest" />
    <None Include="Resources\SWTORTrajan.ttf" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Forms\MainForm.resx">
      <DependentUpon>MainForm.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Forms\MessageDialog.resx">
      <DependentUpon>MessageDialog.cs</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <Content Include="chaos.ico" />
    <None Include="Resources\info.png" />
    <None Include="Resources\launcherImg.png" />
    <None Include="Resources\serverDown.png" />
    <None Include="Resources\serverUp.png" />
    <None Include="Resources\PATH.txt" />
  </ItemGroup>
</Project>