{
  "Logging": {
    "NLog": {
      "extensions": [
        {
          "assembly": "NLog.Targets.Seq"
        }
      ],
      "targets": {
        "logseq": {
          "type": "Seq",
          "serverUrl": "http://localhost:5341",
          "apiKey": "",
          "properties": [
            {
              "name": "Source",
              "value": "${Logger}"
            }
          ]
        }
      },
      "rules": [
        {},
        {
          "logger": "*",
          "minLevel": "Trace",
          "writeTo": "logseq"
        }
      ]
    }
  }
}